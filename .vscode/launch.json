    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch WhatsApp Bot",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/cmd/whatsapp-bot/main.go",
            "cwd": "${workspaceFolder}",
            "env": {
                "CGO_ENABLED": "1",
                "PATH": "${env:PATH};C:\\TDM-GCC-64\\bin"
            }
        },
        {
            "name": "Launch Package",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${fileDirname}",
            "env": {
                "CGO_ENABLED": "1",
                "PATH": "${env:PATH};C:\\TDM-GCC-64\\bin"
            }
        }
    ]
}